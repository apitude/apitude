{
  "name": "apitude/apitude",
  "description": "Yet another API framework",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Gordon Forsythe",
      "email": "baohx2000@gmail.com"
    }
  ],
  "prefer-stable": true,
  "bin": [
    "bin/apiworker",
    "bin/apistackedworker",
    "bin/apicon",
    "bin/apidb"
  ],
  "minimum-stability": "dev",
  "require": {
    "doctrine/orm": "~2",
    "doctrine/migrations": "~1",
    "gedmo/doctrine-extensions": "~2",
    "dflydev/doctrine-orm-service-provider": "~1",
    "silex/silex": "~1",
    "jms/metadata": "~1",
    "dbtlr/silex-doctrine-migrations": "~1",
    "monolog/monolog": "^1.16@dev",
    "asm89/stack-cors": "^0.2",
    "contatta/qless-php": "~1",
    "swiftmailer/swiftmailer": "^5.4",
    "dotblue/mandrill": "^1",
    "xamin/handlebars.php": "^0.10",
    "mailgun/mailgun-php": "^2.0",
    "php-http/guzzle6-adapter": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4",
    "codeception/codeception": "*",
    "symfony/validator": "~2"
  },
  "autoload": {
    "psr-4": {
      "Apitude\\Core\\": "src/"
    }
  }
}
